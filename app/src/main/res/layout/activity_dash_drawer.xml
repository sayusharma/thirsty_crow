<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/overlayBackground"
    tools:context=".DashDrawerActivity">
    <com.shrikanthravi.customnavigationdrawer2.widget.SNavigationDrawer
        android:layout_width="match_parent"
        android:id="@+id/navigationDrawer"
        app:navigationDrawerBackgroundColor="#FFFFFF"
        app:appbarColor="@color/colorPrimary"
        app:appbarTitleTextColor="#FFFFFF"
        app:HamMenuIconTintColor="@color/White"
        app:primaryMenuItemTextColor="#000"
        app:secondaryMenuItemTextColor="#000000"
        android:layout_height="match_parent">
        <FrameLayout
            android:id="@+id/frameLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </com.shrikanthravi.customnavigationdrawer2.widget.SNavigationDrawer>

</androidx.constraintlayout.widget.ConstraintLayout>